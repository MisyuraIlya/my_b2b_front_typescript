{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@reduxjs/toolkit": "^1.9.2",
    "@tanstack/react-query": "^4.29.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.5.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.3.4",
    "babel-jest": "^29.4.3",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "moment-timezone": "^0.5.43",
    "react": "^18.2.0",
    "react-calendar": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-icons": "^4.8.0",
    "react-loader-spinner": "^5.3.4",
    "react-pro-sidebar": "^1.0.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.2",
    "react-useanimations": "^2.10.0",
    "redux-persist": "^6.0.0",
    "sweetalert2": "^11.7.3",
    "swiper": "^9.2.0",
    "ts-jest": "^29.0.5",
    "use-debounce": "^9.0.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "webpack --mode=production --node-env=production",
    "test": "craco test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "eject": "craco eject",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "jest": "jest",
    "wdio": "wdio run ./wdio.conf.ts",
    "test:e2e": "npx wdio run ./wdio.conf.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/js-cookie": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@wdio/cli": "^8.5.6",
    "@wdio/local-runner": "^8.5.6",
    "@wdio/mocha-framework": "^8.5.6",
    "@wdio/spec-reporter": "^8.4.0",
    "@webpack-cli/generators": "^3.0.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromedriver": "^110.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.34.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "jest": "^29.4.3",
    "loki": "^0.31.0",
    "prop-types": "^15.8.1",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wdio-chromedriver-service": "^8.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "description": "My webpack project",
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^@models/(.*)$": "<rootDir>/src/models/$1",
      "^@modules/(.*)$": "<rootDir>/src/modules/$1",
      "^@pages/(.*)$": "<rootDir>/src/pages/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@store/(.*)$": "<rootDir>/src/store/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1"
    }
  }
}
